const demoPair = [39.219785, -106.80139];

const getAllBusinesses = async (city, gridLength, businessType) => {
  const boundingBox = fetchCoordinates(city);
  // create coordinate pairs based on grid lentgh
  const searchGrid = getCoordinates(boundingBox, gridLength);

  searchGrid.forEach((coordinatePair) => {
    fetchBusiness(coordinatePair, businessType);
  });
};
